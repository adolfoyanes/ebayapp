<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Upc</th>
      <th>Current price</th>
      <th>Average price</th>
      <th>Cost</th>
      <th>Gross margin</th>
      <th>Net margin</th>
      <th>Roi</th>
      <th>Name</th>
      <th>Total sold</th>
      <th>Average sold</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.url %></td>
        <td><%= product.upc %></td>
        <td><%= product.current_price %></td>
        <td><%= product.average_price %></td>
        <td><%= product.cost %></td>
        <td><%= product.gross_margin %></td>
        <td><%= product.net_margin %></td>
        <td><%= product.roi %></td>
        <td><%= product.name %></td>
        <td><%= product.total_sold %></td>
        <td><%= product.average_sold %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
