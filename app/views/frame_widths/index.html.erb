<p id="notice"><%= notice %></p>

<h1>Frame Widths</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @frame_widths.each do |frame_width| %>
      <tr>
        <td><%= frame_width.name %></td>
        <td><%= link_to 'Show', frame_width %></td>
        <td><%= link_to 'Edit', edit_frame_width_path(frame_width) %></td>
        <td><%= link_to 'Destroy', frame_width, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Frame Width', new_frame_width_path %>
