<p id="notice"><%= notice %></p>

<h1>Ebay Auths</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Access token</th>
      <th>Refresh token</th>
      <th>Expiration</th>
      <th>Token type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ebay_auths.each do |ebay_auth| %>
      <tr>
        <td><%= ebay_auth.user_id %></td>
        <td><%= ebay_auth.access_token %></td>
        <td><%= ebay_auth.refresh_token %></td>
        <td><%= ebay_auth.expiration %></td>
        <td><%= ebay_auth.token_type %></td>
        <td><%= link_to 'Show', ebay_auth %></td>
        <td><%= link_to 'Edit', edit_ebay_auth_path(ebay_auth) %></td>
        <td><%= link_to 'Destroy', ebay_auth, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ebay Auth', new_ebay_auth_path %>
